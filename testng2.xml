<suite name="Suite">
	<!-- Define parallel test execution -->
	<listeners>
		<listener class-name="com.tests.MyReport"></listener>
	</listeners>
	<test thread-count="5" name="Test" >
		
		<classes>
			<class name="com.tests.AssertionsDemo"></class>
			<class name="com.tests.AnnotationsDemo"></class>
